package basicjava;
import java.util.*;
import java.util.stream.Stream;
public class streamm {
	public static void main(String args[])
	{
		
	
		Stream.of("a1","a2","a3")
		.findFirst()
		.ifPresent(System.out::println);
		Arrays.stream(new int[] {1,2,3})
		.map(n -> 2 * n + 1)
		.average()
		.ifPresent(System.out::println);
	
	

}


}
