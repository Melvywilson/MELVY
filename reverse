import java.util.Scanner;
class reverse
{
public static void main(String[] args)
{
int a,num,d,r=0;
Scanner nam=new Scanner(System.in);
System.out.println("Enter the number");
a=nam.nextInt();
num=a;
while(num>0)
{
d=num%10;
r=(r*10)+d;
num=num/10;
}
System.out.println("The reverse of no.is"+r);
}
}
