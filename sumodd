import java.util.Scanner;
class sumodd
{
public static void main(String[] args)
{
int a,i,sum=0;
Scanner nam=new Scanner(System.in);
System.out.println("Enter the number");
a=nam.nextInt();
for(i=1;i<=a;i++)
{
     if(a%2!=0)
sum=sum+i;
}
System.out.println("Sum of odd no.s is"+sum);
}
}
    
      
