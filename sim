package basicjava;

 class scc {
	 int eno=101;
	 void display()
	 {
		 System.out.println(eno);
	 }
 }
 class manager extends scc{
	 void display()
	 {
		 System.out.println("manager class");
	 }
 }
 class sim
 {
	 public static void main(String args[])
	 {
		 manager m=new manager();
		 m.display();
	 }

}
