class secondlarge
{
public static void main(String[] args)
{
int a=10,b=30,c=6;
if(a>b&&a>c)
{
System.out.println("The largest no. is "+a);
if(b>c)
System.out.println("The second largest no. is "+b);
else
System.out.println("The second largest no. is "+c);
}
if(b>c&&b>a)
{
System.out.println("The largest no. is "+b);
if(a>c)
System.out.println("The second largest no. is "+a);
else
System.out.println("The second largest no. is "+c);
}
if(c>a&&c>b)
{
System.out.println("The largest no. is "+c);
if(b>a)
System.out.println("The second largest no. is "+b);
else
System.out.println("The second largest no. is "+a);
}
}
}
